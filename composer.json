{
    "name": "chiquitto/soul",
    "description": "Core for php system",
    "type": "library",
    "authors": [
        {
            "name": "Alisson Chiquitto",
            "email": "chiquitto@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chiquitto\\Soul\\": "src/"
        },
