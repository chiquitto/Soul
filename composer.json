        "zendframework/zend-servicemanager": "^3",
        "zendframework/zend-validator": "^2.6",
        "aws/aws-sdk-php": "^3",
        "intervention/image": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "bjyoungblood/bjy-profiler": "@dev"
    }
}
